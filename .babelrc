{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@hooks": "./src/hooks",
          "@services": "./src/services",
          "@constants": "./src/constants",
          "@context": "./src/context",
          "@queries": "./src/queries",
          "@data": "./src/data",
          "@typeDefs": "./src/types",
          "@styles": "./src/styles",
          "@utils": "./src/utils",
          "@HOC": "./src/HOC"
        }
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  
  ]
}
